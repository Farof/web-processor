extends layout

block content

  div(id='debug')
    button(onclick='wp.bootstrap()') bootstrap
    button(onclick='wp.clearBootstrap()') clear

  div(id='objects' class='collection')
    h2 Objects

    div(id='process-list' class='collection-category')
      h3(class='control-container')
        span(class='collection-name') Processes
        span(class='control-add control' onclick='wp.Process.new({})') +

    div(id='view-list' class='collection-category')
      h3(class='control-container')
        span(class='collection-name') Views
        span(class='control-add control' onclick='wp.View.new({})') +

  div(id='content-list')

  div(id='library' class='collection')
    h2 Library

    div(id='library-inputs' class='collection-category')
      h3 Inputs

    div(id='library-operators' class='collection-category')
      h3 Operators

    div(id='library-outputs' class='collection-category')
      h3 Outputs

  script(type='application/javascript' src='/javascripts/libs/uuid.js')
  script(type='application/javascript' src='/javascripts/libs/natives.js')
  script(type='application/javascript' src='/javascripts/libs/Element.js')
  script(type='application/javascript' src='/javascripts/dashboard.js')
  script(type='application/javascript' src='/javascripts/WPObj.js')
  script(type='application/javascript' src='/javascripts/Process.js')
  script(type='application/javascript' src='/javascripts/View.js')
  script(type='application/javascript' src='/javascripts/LibraryItem.js')
  script(type='application/javascript' src='/javascripts/bootstrap.js')